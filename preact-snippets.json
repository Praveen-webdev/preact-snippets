{
  "Preact Functional Component": {
    "prefix": "pfc",
    "body": [
      "import { h } from 'preact';",
      "",
      "function $1() {",
      "  return (",
      "    <div>$2</div>",
      "  );",
      "}",
      "",
      "export default $1;"
    ],
    "description": "Preact Functional Component"
  },
  "Preact Class Component": {
    "prefix": "pcc",
    "body": [
      "import { h, Component } from 'preact';",
      "",
      "class $1 extends Component {",
      "  render() {",
      "    return (",
      "      <div>$2</div>",
      "    );",
      "  }",
      "}",
      "",
      "export default $1;"
    ],
    "description": "Preact Class Component"
  },
